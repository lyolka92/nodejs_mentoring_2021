{
  "compilerOptions": {
	"module": "commonjs",
	"esModuleInterop": true,
	"target": "es6",
	"moduleResolution": "node",
	"sourceMap": true,
	"outDir": "dist",
	"experimentalDecorators": true,
	"emitDecoratorMetadata": true
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true,
	"typeRoots": [
	  "./node_modules/@types"
	]
  },
  "lib": ["es2015"]
}